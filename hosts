[DB_TEST]                         #Данный параметр определяет группу хостов.

test_db ansible_host=10.10.10.10

[DB_PROD]                         #Данный параметр определяет группу хостов.

prod_db ansible_host=10.10.10.20

[DB_ALL:children]                 #Данный параметр определяет родительскую группу со списком дочерних групп (Для выполнения тасков для всех групп)
DB_TEST
DB_PROD
